{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/event-reminders",
      "schedule": "*/1 * * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

